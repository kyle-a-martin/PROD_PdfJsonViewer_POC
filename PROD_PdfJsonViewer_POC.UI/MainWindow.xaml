<Window x:Class="PROD_PdfJsonViewer_POC.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PROD_PdfJsonViewer_POC.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="675" Width="1200">
    <Grid>
        <ToolBar Height="34" VerticalAlignment="Top">
            <Button x:Name="OpenFileButton" Content="Open File" Padding="10 0" HorizontalAlignment="Left"></Button>   
        </ToolBar>
        <WebBrowser x:Name="PdfViewer" Height="625" Width="600" HorizontalAlignment="Left" VerticalAlignment="Bottom"></WebBrowser>
        <TreeView x:Name="JsonViewer" Height="625" Width="600" HorizontalAlignment="Right" VerticalAlignment="Bottom"></TreeView>
    </Grid>
</Window>
